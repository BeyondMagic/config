"mpc seek +1%"
    m:0x50 + c:86
      Super + +

"mpc seek -1%"
    m:0x50 + c:82
      Super + -

"myytt"
    m:0x50 + c:90
      Alt + Super + z

"$HOME/github/scripts/screenshot/screen"
    m:0x10 + c:107
      Printscr

"st -T floatst"
    m:0x50 + c:22
      Super + Back
